<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/black"
    tools:context=".results">


    <Button
        android:id="@+id/results_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="First Sem"
        app:layout_constraintBottom_toTopOf="@+id/results_2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.123"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.73" />

    <Button
        android:id="@+id/results_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Second Sem"
        app:layout_constraintBottom_toTopOf="@+id/results_3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.123"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/results_1"
        app:layout_constraintVertical_bias="0.887" />

    <Button
        android:id="@+id/results_3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Third Sem"
        app:layout_constraintBottom_toTopOf="@+id/results_4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.123"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/results_2" />

    <Button
        android:id="@+id/results_4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Fourth Sem"
        app:layout_constraintBottom_toTopOf="@id/results_5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.123"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/results_3" />

    <Button
        android:id="@+id/results_5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Fifth Sem"
        app:layout_constraintBottom_toTopOf="@id/results_6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.123"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/results_4" />

    <Button
        android:id="@+id/results_6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Sixth Sem"
        app:layout_constraintBottom_toTopOf="@id/results_7"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.123"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/results_5" />

    <Button
        android:id="@+id/results_7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Seventh Sem"
        app:layout_constraintBottom_toTopOf="@id/results_8"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.123"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/results_6" />

    <Button
        android:id="@+id/results_8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Eighth Sem"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.123"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/results_7" />
    <WebView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/results_webview"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"/>

</androidx.constraintlayout.widget.ConstraintLayout>